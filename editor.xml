<application>
  <component name="CodeInsightSettings">
    <EXCLUDED_PACKAGE NAME="android.inputmethodservice.Keyboard.Row" />
  </component>
  <component name="DefaultFont">
    <option name="FONT_SIZE" value="16" />
  </component>
  <component name="EditorSettings">
    <option name="languageBreadcrumbsMap">
      <map>
        <entry key="XML" value="true" />
      </map>
    </option>
    <option name="USE_SOFT_WRAPS" value="CONSOLE" />
    <option name="IS_WHEEL_FONTCHANGE_ENABLED" value="true" />
  </component>
  <component name="JetEditorOptions">
    <option name="donTShowConversionDialog" value="true" />
    <option name="enableJavaToKotlinConversion" value="false" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\b(todo|fixme)\b.*" />
  </component>
</application>