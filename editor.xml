<application>
  <component name="CodeInsightSettings">
    <EXCLUDED_PACKAGE NAME="android.inputmethodservice.Keyboard.Row" />
  </component>
  <component name="CodeVisionSettings">
    <option name="disabledCodeVisionProviderIds">
      <set>
        <option value="vcs.code.vision" />
      </set>
    </option>
  </component>
  <component name="EditorSettings">
    <option name="languageBreadcrumbsMap">
      <map>
        <entry key="XML" value="true" />
      </map>
    </option>
    <option name="USE_SOFT_WRAPS" value="MAIN_EDITOR:CONSOLE" />
    <option name="SOFT_WRAP_FILE_MASKS" value="*.md; *.txt; *.rst; *.adoc" />
    <option name="ENABLE_RENDERED_DOC" value="true" />
    <option name="IS_WHEEL_FONTCHANGE_ENABLED" value="true" />
  </component>
  <component name="InlayHintsSettings">
    <option name="lastViewedProviderKeyId" value="parameter.hints.old" />
    <settingsMapElement>
      <kotlin.kotlin.references.types.hints>
        <Settings>
          <option name="localVariableType" value="true" />
        </Settings>
      </kotlin.kotlin.references.types.hints>
    </settingsMapElement>
  </component>
  <component name="JetEditorOptions">
    <option name="donTShowConversionDialog" value="true" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\b(todo|fixme)\b.*" />
  </component>
</application>